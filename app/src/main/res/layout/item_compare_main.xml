<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:elevation="3dp"
    android:padding="5dp"
    android:layout_margin="3dp"
    app:cardCornerRadius="20dp"
    android:outlineAmbientShadowColor="@android:color/darker_gray">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:gravity="center"
        android:text="Dental Implant"
        android:padding="5dp"
        android:layout_margin="5dp"
        android:shadowColor="@android:color/holo_red_light"
        android:outlineAmbientShadowColor="@android:color/holo_red_light"
        android:outlineSpotShadowColor="@android:color/holo_red_light"
        android:id="@+id/cmain_name"
        />


</androidx.cardview.widget.CardView>